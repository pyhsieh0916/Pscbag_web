                                }
                            });
                               


                    });

                    //orderBag
                    $.ajax({
                        type: "POST",
                        url: "/Home/orderBag",
                        dataType: "text",
                        data: {
                            booth_name: booth_name,
                            booth_num: booth_num,
                            total_price: totalPrice
