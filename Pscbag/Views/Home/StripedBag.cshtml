                        checkoutString += ("product_name=" + this.name + ",price=" + this.price + ",quantity=" + this.quantity + ",booth_name=" + booth_name + ",booth_num=" + booth_num + ",phone=" + phone + ",demo=" + demo);

                        
                        var data = $("#registform").serialize();
                            //update
                            $.ajax({
                                type: "POST",
                                url: "/Home/update",
                                dataType: "text",
                                data: {
                                product_name: this.name,
                                price:  this.price, 
                                quantity: this.quantity,
                                booth_name: booth_name, 
                                booth_num: booth_num, 
                                phone:phone,
                                demo: demo
